{
  "name": "aula",
  "version": "1.7.3",
  "description": "aula: gemeinsam gestalten",
  "author": {
    "name": "Aula App",
    "email": "aula@aula.de",
    "url": "https://aula.de"
  },
  "private": true,
  "keywords": [
    "react",
    "typescript",
    "material",
    "mui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aula-app/aula-frontend"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "dev:local": "vite --config vite.config.local.mts",
    "check-type": "tsc --noEmit",
    "build": "tsc && NODE_OPTIONS=--max-old-space-size=3072 vite build",
    "build-app": "tsc && vite build --mode app",
    "build-single": "vite build --mode single",
    "build-test": "tsc && vite build --mode test",
    "build-bw": "tsc && vite build --mode bw",
    "preview": "vite preview",
    "format": "prettier ./src --write",
    "format:all": "prettier . --write",
    "lint": "eslint ./src",
    "check": "tsc",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "type": "tsc",
    "knip": "knip"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/app": "^7.0.0",
    "@capacitor/core": "^7.2.0",
    "@capacitor/ios": "^7.2.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.4",
    "@mdxeditor/editor": "^3.21.1",
    "@mui/icons-material": "^6.0.2",
    "@mui/material": "^6.0.2",
    "@mui/system": "^6.0.2",
    "@mui/x-date-pickers": "^7.15.0",
    "dayjs": "^1.11.13",
    "eslint": "^9.32.0",
    "i18next": "^23.11.5",
    "notistack": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.45.0",
    "react-hook-form-mui": "^7.2.0",
    "react-i18next": "^14.1.2",
    "react-icons": "^5.2.1",
    "react-markdown": "^9.0.1",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.2",
    "rehype-raw": "^7.0.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.2.0",
    "@playwright/test": "^1.53.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^22.7.5",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "knip": "^5.33.2",
    "prettier": "^3.0.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.20",
    "vite-plugin-svgr": "^4.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}