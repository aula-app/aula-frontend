{
  "name": "aula",
  "version": "0.1",
  "description": "aula - Schule gemeinsam gestalten",
  "author": {
    "name": "Aula App",
    "email": "aula@aula.de",
    "url": "https://aula.de"
  },
  "private": true,
  "keywords": [
    "react",
    "typescript",
    "material",
    "mui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aula-app/aula-frontend"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "check-type": "tsc --noEmit",
    "build": "tsc && vite build",
    "build-branch": "tsc && vite --config vite.branch.config.mts --mode branch build",
    "build-devel": "tsc && vite build --mode devel",
    "build-staging": "tsc && vite build --mode staging",
    "build-single": "vite build --mode single",
    "build-test": "tsc && vite build --mode test",
    "build-bw": "tsc && vite build --mode bw",
    "preview": "vite preview",
    "format": "prettier ./src --write",
    "format:all": "prettier . --write",
    "lint": "eslint ./src --ext .js,.ts,.tsx",
    "check": "tsc",
    "type": "tsc",
    "knip": "knip"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@capacitor/android": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/ios": "^7.0.0",
    "@capacitor/keyboard": "^7.0.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/nunito": "^5.0.6",
    "@fontsource-variable/open-sans": "^5.0.6",
    "@hookform/resolvers": "^3.3.4",
    "@mdxeditor/editor": "^3.21.1",
    "@mui/base": "^5.0.0-beta.4",
    "@mui/icons-material": "^6.0.2",
    "@mui/joy": "^5.0.0-alpha.84",
    "@mui/lab": "^5.0.0-alpha.140",
    "@mui/material": "^6.0.2",
    "@mui/x-data-grid": "^7.15.0",
    "@mui/x-date-pickers": "^7.15.0",
    "@tiptap/core": "^2.11.3",
    "@tiptap/extension-heading": "^2.11.3",
    "@tiptap/extension-image": "^2.11.3",
    "@tiptap/extension-table": "^2.11.3",
    "@tiptap/pm": "^2.11.3",
    "@tiptap/react": "^2.11.3",
    "@tiptap/starter-kit": "^2.11.3",
    "@use-gesture/react": "^10.3.1",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "i18next": "^23.11.5",
    "mui-tiptap": "^1.17.1",
    "notistack": "^3.0.1",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.0.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.45.0",
    "react-hook-form-mui": "^7.2.0",
    "react-i18next": "^14.1.2",
    "react-icons": "^5.2.1",
    "react-markdown": "^9.0.1",
    "react-prosemirror": "^0.2.1",
    "react-router-dom": "^6.14.2",
    "react-swipeable": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "validate.js": "latest",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^7.0.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "latest",
    "@tsconfig/create-react-app": "^2.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^22.7.5",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "latest",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.25.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^16.0.0",
    "knip": "^5.33.2",
    "prettier": "^3.0.0",
    "react-focus-lock": "^2.13.6",
    "typescript": "^5.6.3",
    "vite": "^5.0.12",
    "vite-plugin-svgr": "^4.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
